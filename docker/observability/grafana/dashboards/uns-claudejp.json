{
  "id": null,
  "uid": "uns-claudejp-overview",
  "title": "UNS ClaudeJP Observability",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "API Request Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "req/s"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{handler!~\"/metrics\"}[5m]))",
          "legendFormat": "Requests/s",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "p95 Request Duration",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{handler!~\"/metrics\"}[5m])) by (le))",
          "legendFormat": "p95 latency",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "OCR Success vs Failures (5m)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 9
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["last"],
          "fields": ""
        },
        "orientation": "horizontal",
        "textMode": "value_and_name"
      },
      "targets": [
        {
          "expr": "increase(ocr_requests_total[5m])",
          "legendFormat": "Requests",
          "refId": "A"
        },
        {
          "expr": "increase(ocr_failures_total[5m])",
          "legendFormat": "Failures",
          "refId": "B"
        }
      ]
    },
    {
      "type": "gauge",
      "title": "Error Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 9
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "percentage",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 2 },
              { "color": "red", "value": 5 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      },
      "targets": [
        {
          "expr": "(sum(rate(http_requests_total{status=~\"5..\",handler!~\"/metrics\"}[5m])) / sum(rate(http_requests_total{handler!~\"/metrics\"}[5m]))) * 100",
          "refId": "A"
        }
      ]
    }
  ],
  "templating": {
    "list": []
  }
}
