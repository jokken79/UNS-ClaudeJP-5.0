{
  "version": "1.0",
  "framework": "nextjs",
  "language": "typescript",
  "styling": "tailwindcss",
  "components": {
    "directory": "./components",
    "ui": "./components/ui"
  },
  "theme": {
    "mode": "light",
    "colors": {
      "primary": {
        "default": "#3B82F6",
        "hover": "#2563EB",
        "active": "#1D4ED8"
      },
      "secondary": {
        "default": "#F0F4F8",
        "hover": "#E5E9EF",
        "active": "#D1D8E0"
      },
      "background": "#FFFFFF",
      "foreground": "#080A25",
      "accent": "#6366F1",
      "destructive": "#EF4444",
      "success": "#10B981",
      "warning": "#F59E0B",
      "info": "#3B82F6",
      "border": "#E5E9EF",
      "input": "#E5E9EF",
      "muted": "#F0F4F8"
    },
    "darkMode": {
      "colors": {
        "primary": {
          "default": "#F8FAFC",
          "hover": "#E2E8F0",
          "active": "#CBD5E1"
        },
        "secondary": {
          "default": "#1E2937",
          "hover": "#374151",
          "active": "#4B5563"
        },
        "background": "#080A25",
        "foreground": "#F8FAFC",
        "accent": "#334155",
        "destructive": "#991B1B",
        "success": "#065F46",
        "warning": "#92400E",
        "info": "#1E3A8A",
        "border": "#1E2937",
        "input": "#1E2937",
        "muted": "#1E2937"
      }
    },
    "borderRadius": "0.5rem",
    "fonts": {
      "sans": "Inter, system-ui, sans-serif",
      "mono": "monospace"
    }
  },
  "preview": {
    "port": 3000,
    "open": false
  }
}
